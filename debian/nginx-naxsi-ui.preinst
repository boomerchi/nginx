#!/bin/sh
set -e

case "$1" in
  upgrade)
    # If this is an upgrade, save the current config file, then overwrite it with the new one.
    mv /etc/nginx/naxsi-ui.conf /etc/nginx/naxsi-ui.conf.before_upgrade_to_jessie
    mv /etc/nginx/naxsi-ui.conf.jessie /etc/nginx/naxsi-ui.conf
    ;;
  *)
    echo "preinst called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#

exit 0
